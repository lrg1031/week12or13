   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TextField fx:id="textField1" />
      <TextField fx:id="textField2" />
      <Button fx:id="GCDButton" mnemonicParsing="false" onAction="#computeGCD" text="Find GCD" />
      <Label fx:id="answerLabel" />
   </children>
</VBox>
